<div class="card">
  <%= link_to image_tag('', class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.user.name, user_path(current_user.id), class: :card__user %>
  </div>
</div>
